#include <bits/stdc++.h>
using namespace std;

struct Node{
	int data;
	Node* left,right;
	Node(int x)
	{
		data=x;
		left=right=NULL;
	}

};
int main() {
	Node *root=new Node(10);
	Node *root->left = new Node(20);
	Node *root->rightt = new Node(30);
	Node *root->left->right = new Node(40);
	return 0;
}
