#include<bits/stdc++.h> 
using namespace std;
int main()
{
	pair<int,int>p={1,2};
	//cout<<p.first<<" "<<p.second;
	vector<pair<int,int>>vp={(1,2},{2,3},{3,4}}//vector pair
	for(auto it:vp){
		cout<<it.first<<" "<<it.second<<"\n";
	}
	//map will have a key and a value(similar to dictionary)
	int arr[6]={1,2,1,3,4,3}
	map<int,int>mpp;
	for(int i=0;i<6;i++)
	{
		mpp(arr[i++]);
	}
	for(auto itt:mpp)
	{
		cout<<itt.first<<" "<<itt.second<<endl;
	}

	unordered_map<int,int>unmp; //works faster than map

	//map with pair
	map<int,pair<int,int>mp={1,{2,5}};
	for(auto i:mp)
	{
		cout<<i.first<<" "<<i.second.first<<endl;
	}
	map<int,int>d;
	//stores data in sorted ,sorting will be done on keys
	map<int,pair<int,int>>d1;//one key may have set of pairs
	map<pair<int,int>,int>d2;//set of keys may have one value

	d[1]=2;//d{{1,2}}
	d.emplace({3,1});
	d.insert({2,4});

	{{1,2},{3,1},{2,4}}

	for(auto it:d)
	{
		cout<<it.first<<" "<<it.second;
	}

	d2[{2,3}]=10;//set of keys have one value
cout<<d[2];//prints 4
	
	}

